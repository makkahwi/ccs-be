{
  "kind": "collectionType",
  "collectionName": "currencies",
  "info": {
    "singularName": "currency",
    "pluralName": "currencies",
    "displayName": "Currency",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "mostRecentEdition": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "fractionName": {
      "type": "string",
      "required": true
    },
    "fraction": {
      "type": "integer",
      "required": true
    },
    "country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::country.country",
      "inversedBy": "currencies"
    },
    "exchange_rate": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::exchange-rate.exchange-rate",
      "mappedBy": "currency"
    },
    "editions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::currency-edition.currency-edition",
      "mappedBy": "currency"
    }
  }
}
